#include <stdio.h>
#include <stdlib.h>

int min(int* a, int* b) {
    if (*a < *b) {
        return *a;
    } else {
        return *b;
    }
}

void zad3_2_1() {
    int x = 10;
    int y = 20;
    int z = min(&x, &y); // z = 10
    printf("Mniejsza wartosc to: %d\n", z);
    return;
}


int main()
{
    zad3_2_1();
    return 0;
}
