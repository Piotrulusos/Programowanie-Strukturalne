#include <stdio.h> //xd
#include <stdlib.h>

int main() {
    int a, b;
    printf("Podaj dwie liczby calkowite: ");
    scanf("%d %d", &a, &b);
    if (abs(a) >= abs(b)) {
        printf("%d ma wieksza wartosc bezwzgledna\n", a);
    } else {
        printf("%d ma wieksza wartosc bezwzgledna\n", b);
    }
    return 0;
}
