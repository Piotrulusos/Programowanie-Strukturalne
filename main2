#include <stdio.h>

int main() {
    int liczba1, liczba2, wieksza;

    printf("Podaj pierwsza liczbe: ");
    scanf("%d", &liczba1);

    printf("Podaj druga liczbe: ");
    scanf("%d", &liczba2);

    // Wybieramy wiÄ™ksza wartosc bezwzgledna
    if (abs(liczba1) > abs(liczba2)) {
        wieksza = liczba1;
    } else {
        wieksza = liczba2;
    }

    printf("Liczba o wiekszej wartosci bezwzglednej to: %d\n", wieksza);

    return 0;
}
