#include <stdio.h>

int main() {
    int n, m, i, gcd;
    printf("Podaj dwie dodatnie liczby całkowite n i m: ");
    scanf("%d %d", &n, &m);
    
    // Szukamy największego wspólnego dzielnika
    for (i = 1; i <= n && i <= m; i++) {
        if (n % i == 0 && m % i == 0) {
            gcd = i;
        }
    }
    
    printf("Największy wspólny dzielnik liczby %d i %d to %d\n", n, m, gcd);
    return 0;
}
