#include <stdio.h>

int main() {
    int n, i;
    long long sum = 0;
    
    printf("Podaj nieujemna liczbe calkowita n: ");
    scanf("%d", &n);
    
    for (i = 0; i <= n; i++) {
        sum += i * i;
    }
    
    printf("Suma kwadratow od 0 do %d wynosi: %lld\n", n, sum);
    
    return 0;
}
