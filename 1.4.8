#include <stdio.h>

int main() {
    int n, i;
    int f0 = 0, f1 = 1, fn;

    printf("Podaj nieujemna liczbe ca≈Çkowita n: ");
    scanf("%d", &n);

    if (n == 0) {
        fn = f0;
    } else if (n == 1) {
        fn = f1;
    } else {
        for (i = 2; i <= n; i++) {
            fn = f0 + f1;
            f0 = f1;
            f1 = fn;
        }
    }

    printf("Element ciagu Fibonacciego o indeksie %d wynosi: %d\n", n, fn);

    return 0;
}
