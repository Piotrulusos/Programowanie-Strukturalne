#include <stdio.h>

int main() {
    int a, b, c;
    printf("Podaj trzy liczby calkowite: ");
    scanf("%d %d %d", &a, &b, &c);
    if (a >= b && a >= c) {
        printf("%d jest najwieksze\n", a);
    } else if (b >= a && b >= c) {
        printf("%d jest najwieksze\n", b);
    } else {
        printf("%d jest najwieksze\n", c);
    }
    return 0;
}
